{
  "name": "wav2lip",
  "version": "1.0.0",
  "description": "wav2lip is a docker wrapper over wav2lip",
  "main": "index.js",
  "directories": {
    "example": "example"
  },
  "scripts": {
    "test": "npm run test",
    "docker-example-cpu": "docker run -v \"$(pwd)\"/example:/data -v \"$(pwd)\"/result:/usr/wav2lip/results 0x4139/wav2lip:v1 python inference.py --checkpoint_path checkpoints/wav2lip.pth --face /data/game.mp4 --audio /data/game.wav",
    "docker-build": "docker buildx build -t docker.io/0x4139/wav2lip:latest --push -f Dockerfile --platform=linux/amd64 ."
  },
  "repository": {
    "type": "git",
    "url": "github.com/0x4139/wav2lip"
  },
  "keywords": [
    "wav2lip",
    "lip-syncing"
  ],
  "author": "Vali Malinoiu <0x4139@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "dockerode": "^3.3.4"
  }
}
